@model IEnumerable<FilmDude2.Models.Movies>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-bordered table-hover">
    @{ foreach (var m in Model)
        {
            <tr>
                <td>@m.Name</td>
            </tr>
        }
    }
</table>




